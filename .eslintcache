alse)\n      }\n      timer(60);\n    }\n\n  }\n\n\n\n  const onSubmit = (value) => {\n\n    setShowLoad(false)\n    setLoadState(true)\n    let formData = new FormData();\n    formData.append(\"user_id\", value.p_email);\n    formData.append(\"password\", value.p_password);\n\n    axios({\n      method: \"POST\",\n      url: `${baseUrl}/customers/login`,\n      data: formData,\n    })\n      .then(function (response) {\n        console.log(\"res-\", response);\n        if (response.data.code === 1) {\n          setShowLoad(true)\n          setLoadState(false)\n          Alerts.SuccessNormal(\"As per your request , OTP has been sent to your email address.\")\n          setShow(true)\n          setLoad(true)\n          setUid(response.data.user_id)\n        } else if (response.data.code === 0) {\n          Alerts.ErrorNormal(\"Invalid email or password.\")\n          setShowLoad(true)\n          setLoadState(false)\n        }\n      })\n      .catch((error) => {\n        console.log(\"erroror - \", error);\n      });\n  };\n\n\n  const handleChange = (e) => {\n    console.log(\"val-\", e.target.value);\n    setEmail(e.target.value);\n  };\n\n  // const upDateTime = (key) => {\n  //   console.log(key)\n  // setLoad(key);\n  // }\n  return (\n    <>\n\n      <Header noSign=\"noSign\" />\n\n      <h1 style={{ \"textAlign\": \"center\", \"margin\": \"55px 0 30px 0\" }}>\n        Would you like to post a query\n      </h1>\n      <div className=\"loading\">      \n        <BarLoader size={72} color=\"orange\" loading={loadState} />\n      </div>\n      {showLoad === true ? <div className=\"StartPage\">\n        <div className=\"mainContent\">\n\n          <div className=\"signIn\">\n            <div className=\"signBtn\">\n              <div className=\"boxOverlay\">\n                <Typography variant=\"h4\" style={{ \"margin\": \"5px auto\", \"color\": \"#fff\" }}>\n                  For new customers\n                </Typography>\n                <button className=\"